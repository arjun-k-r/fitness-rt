[
  {
    "group": "Arms",
    "exercises": [
      {
        "image": "assets/images/curls.jpg",
        "name": "Curls",
        "instructions": [
          "Sit holding a dumbbell in each hand with arms hanging down and the palms of the hands Lacing the body",
          "Inhale and bend the the elbow, rotating the palm up before the forearm reaches horizontal",
          "Continue by raising the elbows at the end of the movement"
        ],
        "variations": [
          "Emphasize biceps",
          "Work brachioradialis intensely",
          "Work mainly biceps and brachialis"
        ],
        "comments": [
          "This exercise primarily uses the brachioradialis (long supinator), brachial's, biceps brachii, and anterior deltoid and, to a lesser extent, the coracobrachialis and clavicular head of the pectoral's major"
        ],
        "muscles": [
          "Biceps brachii",
          "Brachialis",
          "Brachioardialis",
          "Anterior deltoid"
        ],
        "warning": ""
      },
      {
        "image": "",
        "name": "Concentration curls",
        "instructions": [
          "Sit holding a dumbbell with the palm facing forward and the elbow positioned against the inner thigh",
          "Inhale and lift the forearm by bending the elbow",
          "Exhale at the end of the effort"
        ],
        "variations": [],
        "muscles": [
          ""
        ],
        "comments": [
          "This isolation exercise allows you to control the range of motion, speed. and form of the movement",
          "It mainly works the biceps brachii and brachialis"
        ],
        "warning": ""
      },
